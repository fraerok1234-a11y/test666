{
	"expo": {
		"name": "Тестовое Приложение",
		"slug": "test-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#f8f9fa"
		},
		"assetBundlePatterns": [
			"**/*"
		],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.testapp.ios",
			"buildNumber": "1",
			"infoPlist": {
				"CFBundleDisplayName": "Тестовое Приложение",
				"CFBundleName": "TestApp",
				"UIStatusBarStyle": "dark-content",
				"UIViewControllerBasedStatusBarAppearance": false,
				"NSAppTransportSecurity": {
					"NSAllowsArbitraryLoads": true
				}
			},
			"appStoreUrl": "https://apps.apple.com/app/testapp",
			"marketingUrl": "https://testapp.com"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#f8f9fa"
			},
			"package": "com.testapp.android",
			"versionCode": 1,
			"permissions": [],
			"playStoreUrl": "https://play.google.com/store/apps/details?id=com.testapp.android"
		},
		"web": {
			"favicon": "./assets/favicon.png",
			"bundler": "metro"
		},
		"plugins": [],
		"scheme": "testapp",
		"platforms": [
			"ios",
			"android",
			"web"
		]
	}
}